<?xml version="1.0" encoding='UTF-8'?>
<OPTIONS>
   <PACKAGE NAME="Beam">

      <OPT NAME="Note Bubble Options" TYPE="DIVIDER">
	 <TAB NAME="Popular Options"/>
	 <TAB NAME="Bubble Config"/>
      </OPT>
      <OPT NAME="Beam.note.width" DESCRIPTION="Default note bubble width" TYPE="INTEGER">
	 <TAB NAME="Popular Options"></TAB>
	 <TAB NAME="Bubble Config"></TAB>
	 <EXAMPLE TEXT="Width in pixels"></EXAMPLE>
	 <KEYWORD TEXT="Size"/>
      </OPT>
      <OPT NAME="Beam.note.height" DESCRIPTION="Default note bubble height" TYPE="INTEGER">
	 <TAB NAME="Popular Options"></TAB>
	 <TAB NAME="Bubble Config"></TAB>
	 <EXAMPLE TEXT="Height in pixels"></EXAMPLE>
	 <KEYWORD TEXT="Bubble"/>
	 <KEYWORD TEXT="Note"/>
	 <KEYWORD TEXT="Size"/>
	 <KEYWORD TEXT="Height"/>
      </OPT>
      <OPT NAME="Beam.note.top.color" DESCRIPTION="Top color of note bubble" TYPE="COLOR">
	 <TAB NAME="Popular Options"/>
	 <KEYWORD TEXT="Bubble"/>
	 <KEYWORD TEXT="Note"/>
	 <KEYWORD TEXT="Color"/>
	 <KEYWORD TEXT="Top"/>
      </OPT>
      <OPT NAME="Beam.note.bottom.color" DESCRIPTION="Bottom color of note bubble" TYPE="COLOR">
	 <TAB NAME="Popular Options"/>
	 <KEYWORD TEXT="Bubble"/>
	 <KEYWORD TEXT="Note"/>
	 <KEYWORD TEXT="Color"/>
	 <KEYWORD TEXT="Bottom"/>
      </OPT>
      <OPT NAME="Beam.note.font.size" DESCRIPTION="Default size of the font in notes" TYPE="INTEGER" MIN="4" MAX="64" SLIDER="TRUE" NEGATIVEALLOWED="FALSE">
	 <TAB NAME="Font Options"/>
	 <EXAMPLE TEXT="Minimum 4, max 64"/>
      </OPT>

      <OPT NAME="Problem/TODO Bubble Options" TYPE="DIVIDER">
	 <TAB NAME="Popular Options"/>
	 <TAB NAME="Bubble Config"/>
      </OPT>
      <OPT NAME="Beam.problem.width" DESCRIPTION="Default problem bubble width" TYPE="INTEGER">
	 <TAB NAME="Popular Options"></TAB>
	 <TAB NAME="Bubble Config"></TAB>
	 <EXAMPLE TEXT="Width in pixels"></EXAMPLE>
	 <KEYWORD TEXT="Bubble"/>
	 <KEYWORD TEXT="Problem"/>
	 <KEYWORD TEXT="Size"/>
	 <KEYWORD TEXT="Width"/>
      </OPT>
      <OPT NAME="Beam.problem.height" DESCRIPTION="Default problem bubble height" TYPE="INTEGER">
	 <TAB NAME="Popular Options"></TAB>
	 <TAB NAME="Bubble Config"></TAB>
	 <EXAMPLE TEXT="Height in pixels"></EXAMPLE>
	 <KEYWORD TEXT="Bubble"/>
	 <KEYWORD TEXT="Problem"/>
	 <KEYWORD TEXT="Size"/>
	 <KEYWORD TEXT="Height"/>
      </OPT>
      <OPT NAME="Beam.problem.top.color" DESCRIPTION="Top color of problem bubble" TYPE="COLOR">
	 <TAB NAME="Popular Options"/>
	 <KEYWORD TEXT="Bubble"/>
	 <KEYWORD TEXT="Problem"/>
	 <KEYWORD TEXT="Color"/>
	 <KEYWORD TEXT="Top"/>
      </OPT>
      <OPT NAME="Beam.problem.bottom.color" DESCRIPTION="Bottom color of problem bubble" TYPE="COLOR">
	 <TAB NAME="Popular Options"/>
	 <KEYWORD TEXT="Bubble"/>
	 <KEYWORD TEXT="Problem"/>
	 <KEYWORD TEXT="Color"/>
	 <KEYWORD TEXT="Bottom"/>
      </OPT>

   </PACKAGE>

   <PACKAGE NAME="System">
      <OPT NAME="Load Options" TYPE="DIVIDER">
	 <TAB NAME="System Options"/>
      </OPT>
      <OPT NAME="edu.brown.cs.bubbles.ask_workspace" DESCRIPTION="Always request workspace on load" TYPE="BOOLEAN">
	 <TAB NAME="System Options"/>
	 <WARNING TEXT="Requires restart"/>
      </OPT>
      <OPT NAME="edu.brown.cs.bubbles.foreground" DESCRIPTION="Load eclipse in the foreground" TYPE="BOOLEAN">
	 <TAB NAME="System Options"/>
	 <WARNING TEXT="Requires restart"/>
      </OPT>
      <OPT NAME="edu.brown.cs.bubbles.vmsize" DESCRIPTION="Maximum memory size for the java VM to use" TYPE="INTEGER" MIN="256" MAX="4000" SLIDER="TRUE" NEGATIVEALLOWED="FALSE">
	 <TAB NAME="System Options"/>
	 <EXAMPLE TEXT="Size in MB"/>
	 <WARNING TEXT="Requires restart"/>
      </OPT>
      <OPT NAME="edu.brown.cs.bubbles.ask_workspace" DESCRIPTION="Always ask what workspace to open on Startup" TYPE="BOOLEAN">
	 <TAB NAME="System Options"/>
	 <WARNING TEXT="If turned off, will use current workspace by default"/>
      </OPT>
      <OPT NAME="edu.brown.cs.bubbles.use_stderr" DESCRIPTION="Print errors/information to stderr" TYPE="BOOLEAN">
	 <TAB NAME="System Options"/>
      </OPT>
      <OPT NAME="edu.brown.cs.bubbles.log_level" DESCRIPTION="Level of information to log" TYPE="COMBO">
	 <TAB NAME="System Options"/>
	 <COMBO TEXT="Errors only" VALUE="ERROR"/>
	 <COMBO TEXT="Above and warnings" VALUE="WARNING"/>
	 <COMBO TEXT="Above and information" VALUE="INFO"/>
	 <COMBO TEXT="Above and debug information" VALUE="DEBUG"/>
      </OPT>
      <OPT NAME="Board.blur.screenshot" DESCRIPTION="Enable screenshot bluring" TYPE="BOOLEAN">
	 <TAB NAME="System Options"/>
      </OPT>
   </PACKAGE>




   <PACKAGE NAME="Bale">
      <OPT NAME="Code Highlight Options" TYPE="DIVIDER">
