      <OPT NAME="Visualization Icon Options" TYPE="DIVIDER">
	 <TAB NAME="Visualization Options"/>
      </OPT>
      <OPT NAME="visualization.icon.enable" DESCRIPTION="Enable visualization Icons" TYPE="BOOLEAN">
	 <TAB NAME="Visualization Options"/>
	 <KEYWORD TEXT="visualization"/>
	 <KEYWORD TEXT="icon"/>
	 <KEYWORD TEXT="enable"/>
      </OPT>
      <OPT NAME="visualization.icon.indication" DESCRIPTION="You want icons indicate" TYPE="COMBO">
	 <TAB NAME="Visualization Options"/>
	 <COMBO TEXT="Class" VALUE="class"/>
	 <COMBO TEXT="Package" VALUE="package"/>
	 <EXAMPLE TEXT="class or package"></EXAMPLE>
	 <KEYWORD TEXT="visualization"/>
	 <KEYWORD TEXT="icon"/>
	 <KEYWORD TEXT="indicate"/>
      </OPT>
      <OPT NAME="visualization.icon.transparent" DESCRIPTION="The transparency of icons" TYPE="INTEGER" MAX="100" MIN="0" SLIDER="true">
	 <TAB NAME="Visualization Options"/>
	 <EXAMPLE TEXT="0~100"></EXAMPLE>
	 <KEYWORD TEXT="visualization"/>
	 <KEYWORD TEXT="icon"/>
	 <KEYWORD TEXT="transparent"/>
      </OPT>
      <OPT NAME="visualization.icon.size" DESCRIPTION="The size of icons" TYPE="COMBO">
	 <TAB NAME="Visualization Options"/>
	 <COMBO TEXT="Small" VALUE="small"/>
	 <COMBO TEXT="Large" VALUE="large"/>
	 <EXAMPLE TEXT="Switch between large and small icons"></EXAMPLE>
	 <KEYWORD TEXT="visualization"/>
	 <KEYWORD TEXT="icon"/>
	 <KEYWORD TEXT="size"/>
	 <KEYWORD TEXT="small"/>
	 <KEYWORD TEXT="large"/>
      </OPT>
      <OPT NAME="visualization.icon.location" DESCRIPTION="The location of icons" TYPE="COMBO">
	 <TAB NAME="Visualization Options"/>
	 <COMBO TEXT="Center" VALUE="center"/>
	 <COMBO TEXT="Top right" VALUE="topright"/>
	 <COMBO TEXT="Top left" VALUE="topleft"/>
	 <COMBO TEXT="Bottom right" VALUE="bottomright"/>
	 <COMBO TEXT="Bottom left" VALUE="bottomleft"/>
	 <KEYWORD TEXT="visualization"/>
	 <KEYWORD TEXT="icon"/>
	 <KEYWORD TEXT="location"/>
      </OPT>
      <OPT NAME="Visualization Gradient Options" TYPE="DIVIDER">
	 <TAB NAME="Visualization Options"/>
      </OPT>
      <OPT NAME="visualization.gradient.enable" DESCRIPTION="Enable visualization gradient background" TYPE="BOOLEAN">
	 <TAB NAME="Visualization Options"/>
	 <KEYWORD TEXT="visualization"/>
	 <KEYWORD TEXT="gradient"/>
	 <KEYWORD TEXT="background"/>
	 <KEYWORD TEXT="enable"/>
      </OPT>
      <OPT NAME="visualization.gradient.indication" DESCRIPTION="You want different gradient backgrounds indicate" TYPE="COMBO">
	 <TAB NAME="Visualization Options"/>
	 <COMBO TEXT="Class" VALUE="class"/>
	 <COMBO TEXT="Package" VALUE="package"/>
	 <EXAMPLE TEXT="class or package"></EXAMPLE>
	 <KEYWORD TEXT="visualization"/>
	 <KEYWORD TEXT="gradient"/>
	 <KEYWORD TEXT="indicate"/>
      </OPT>
      <OPT NAME="visualization.gradient.direction" DESCRIPTION="The direction of gradient " TYPE="COMBO">
	 <TAB NAME="Visualization Options"/>
	 <COMBO TEXT="Horizontal" VALUE="horizontal"/>
	 <COMBO TEXT="Vertical" VALUE="vertical"/>
	 <COMBO TEXT="Skew" VALUE="skew"/>
	 <EXAMPLE TEXT="vertical, horizontal or skew"></EXAMPLE>
	 <KEYWORD TEXT="visualization"/>
	 <KEYWORD TEXT="gradient"/>
	 <KEYWORD TEXT="direction"/>
      </OPT>
      <OPT NAME="Code Bubble Options" TYPE="DIVIDER">
	 <TAB NAME="Bubble Config"/>
      </OPT>
      <OPT NAME="Bale.comment.wrapping" DESCRIPTION="Comment Wrapping" TYPE="BOOLEAN">
	 <TAB NAME="Bubble Config"/>
	 <TAB NAME="Popular Options"/>
	 <WARNING TEXT="Requires restart"/>
      </OPT>
      <OPT NAME="Bale.autocomplete.delay" DESCRIPTION="Autocomplete Delay" TYPE="INTEGER">
	 <TAB NAME="Bubble Config"/>
	 <KEYWORD TEXT="autocomplete"/>
	 <KEYWORD TEXT="delay"/>
      </OPT>
      <OPT NAME="Bale.crumb.showmethod" DESCRIPTION="Show method name in breadcrumb bar" TYPE="BOOLEAN">
	 <TAB NAME="Bubble Config"/>
	 <KEYWORD TEXT="breadcrumb"/>
	 <KEYWORD TEXT="method"/>
      </OPT>
      <OPT NAME="Bale.editor.no.elision" DESCRIPTION="Shouldn't do code ellisions" TYPE="BOOLEAN">
	 <TAB NAME="Bubble Config"/>
	 <KEYWORD TEXT="elision"/>
	 <KEYWORD TEXT="elide"/>
	 <KEYWORD TEXT="code"/>
      </OPT>
      <OPT NAME="Bale.preview.enable" DESCRIPTION="Show Bubble when Hovering" TYPE="BOOLEAN">
	 <TAB NAME="Bubble Config"/>
	 <EXAMPLE TEXT="Show preview bubble when hovering on java element(method)"></EXAMPLE>
	 <KEYWORD TEXT="preview"/>
	 <KEYWORD TEXT="hover"/>
      </OPT>
      <OPT NAME="Bale.size" DESCRIPTION="Size of code font" TYPE="INTEGER" MIN="8" MAX="20" SLIDER="TRUE" NEGATIVEALLOWED="FALSE">
	 <TAB NAME="Font Options"/>
	 <KEYWORD TEXT="point"/>
	 <KEYWORD TEXT="style"/>
	 <EXAMPLE TEXT="Minimum size 8, maximum 20"/>
	 <WARNING TEXT="Requires restart"/>
      </OPT>
   </PACKAGE>

   <PACKAGE NAME="Buda">
      <OPT NAME="Panning Bar Options" TYPE="DIVIDER">
	 <TAB NAME="Visualization Options"/>
      </OPT>
      <OPT NAME="Buda.overview.stylized.viewport" DESCRIPTION="Show Stylized Viewport in Panning Bar" TYPE="BOOLEAN">
	 <TAB NAME="Visualization Options"/>
	 <KEYWORD TEXT="viewport"/>
	 <KEYWORD TEXT="panning"/>
      </OPT>
      <OPT NAME="Buda.help.tooltips" TYPE="Boolean" DESCRIPTION="Show help tooltips">
	 <TAB NAME="Bubble Config"/>
	 <TAB NAME='Popular Options'/>
	 <TAB NAME='System Options'/>
	 <KEYWORD TEXT='help' />
      </OPT>
      <OPT NAME="Buda.mergeforreg" DESCRIPTION="Show merged search on right click" TYPE="BOOLEAN">
	 <TAB NAME="Search Options"/>
      </OPT>
      <OPT NAME="Buda.show.potential.task" DESCRIPTION="Show potential Working Set on Panning Bar" TYPE="BOOLEAN">
	 <TAB NAME="Visualization Options"/>
	 <KEYWORD TEXT="panning"/>
      </OPT>
      <OPT NAME="Buda.shelf.by.date" DESCRIPTION="Sort Task Shelf:" TYPE="COMBO">
	 <TAB NAME="Visualization Options"/>
	 <COMBO TEXT="Alphabetically" VALUE="false"/>
	 <COMBO TEXT="By Date" VALUE="true"/>
	 <KEYWORD TEXT="task"/>
	 <KEYWORD TEXT="shelf"/>
      </OPT>

   </PACKAGE>

   <PACKAGE NAME="Bgta">
      <OPT NAME="Chat Visualization Options" TYPE="DIVIDER">
	 <TAB NAME="Visualization Options"/>
      </OPT>
      <OPT NAME="Bgta.altcolor" DESCRIPTION="When recieving new chats show chat bubble differently" TYPE="BOOLEAN">
