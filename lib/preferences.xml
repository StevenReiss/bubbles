	 <KEYWORD TEXT="variable"/>
	 <KEYWORD TEXT="write"/>
	 <KEYWORD TEXT="change"/>
	 <KEYWORD TEXT="highlight"/>
      </OPT>
      <OPT NAME="Bale.identifier.define.highlight.color" DESCRIPTION="Color for highlighting the first definition of identifiers" TYPE="COLOR">
