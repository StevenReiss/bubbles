	 <KEYWORD TEXT="identifier"/>
	 <KEYWORD TEXT="variable"/>
	 <KEYWORD TEXT="normal"/>
	 <KEYWORD TEXT="highlight"/>
      </OPT>
      <OPT NAME="Bale.identifier.write.highlight.color" DESCRIPTION="Color for highlighting changing identifiers" TYPE="COLOR">
